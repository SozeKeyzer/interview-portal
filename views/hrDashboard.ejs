    function cancelInterview(uniqueId){
        console.log(uniqueId);
        
           fetch(`/cancelInterview?uniqueId=${uniqueId}`)
            .then(response => response.text())
            .then(data => {
                const cancel_info=document.getElementById("cancel_info");
                const cancel_button=document.getElementById("cancel_button");
                console.log(data); // Output: Hello, world!
                if(data=="f"){
                    cancel_button.setAttribute("onClick",`cancelled('${uniqueId}','full')`);
                    cancel_info.innerHTML=`<p>Your are cancelling this Interview after 12 hours from the time it was scheduled. You will still be charged with full payment</p>
