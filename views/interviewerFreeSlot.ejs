<script>
    $(function() {

        $( "#datepicker" ).datepicker({
            minDate: new Date(), // set minimum date to today
            maxDate: "+2w", // set maximum date to 1 week from today
            dateFormat: "d MM, yy"
        })

        $( "#datepicker1" ).datepicker({
            minDate: new Date(), // set minimum date to today
            maxDate: "+2w", // set maximum date to 1 week from today
            dateFormat: "d MM, yy",
            onSelect: function(selectedDate) {
            // calculate the date 6 days from the selected date to get the end date of the week
                var endDate = new Date(selectedDate);
                endDate.setDate(endDate.getDate() + 6);
                // set the end date in the datepicker
                $( "#datepicker2" ).datepicker( "option", "minDate", selectedDate );
                $( "#datepicker2" ).datepicker( "option", "maxDate", endDate );
                // set the selected date as the start date in the datepicker
                $(this).val(selectedDate);
            }
        });
	   
        // initialize second datepicker with no initial date range set
        $( "#datepicker2" ).datepicker({
            minDate: new Date(),
            maxDate: "+1w",
            dateFormat: "d MM, yy"
        });

        $('.timepicker').timepicker({

            interval: 30,
            minTime: '8:00 am',
            maxTime: '9:00 pm',
            // startTime: '8:00 am',
            timeFormat: 'h:i a',
            dynamic: true,
            dropdown: true,
            scrollbar: true
        });
    });
</script>
