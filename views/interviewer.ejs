  <body>
    <h1 style="text-align: center;">Entrevue</h1>
    <p>Please join the video conference below:<a href="<%=link%>"><%= link %></a></p>
    
    

    <div class="upper">

        <div class="realTime">
            <div class="realTime-header">
                <h2 style="font-weight: 900;">Real-Time Feedback Form</h2>
            </div>

            <div class="card">
                <div class="card-body">
                    <form action="">
                        <div class="form-group row">
                            <label class="col-lg-3 col-form-label">Name : </label>
                            <div class="col-lg-9">    
                                <input type="text" class="form-control" readonly id="name" name="name">
                            </div>
                        </div>
                        
                        
                        
                        <div class="form-group row">
                           
                            <label class="col-lg-6 col-form-label">Role : </label>
                            <div class="col-lg-6">    
                                <input type="text" class="form-control" readonly id="role" name="role">
                            </div>
                        </div>
                        
                        <div class="form-group row">
                            <label class="col-lg-6 col-form-label">Interview Date : </label>
                            <div class="col-lg-6">    
                                <input type="text" class="form-control" readonly id="date" name="date">
                            </div>
                        </div>

                        <div class="form-group row">
                            <label class="col-lg-6 col-form-label">Interview Time : </label>
                            <div class="col-lg-6">    
                                <input type="text" class="form-control" readonly id="time" name="time">
                            </div>
                        </div>

                        <div class="form-group row">
                            <label class="col-lg-6 col-form-label">Coding Language : </label>
                            <div class="col-lg-6">    
                                <!-- <input type="text" class="form-control" readonly value="Coding Language"> -->
                                <select class="form-control codingLanguage" name="codingLanguage" id="codingLanguage" >
                                    <option value="<%=realTimeFeedbackData.codingLanguage%>" disabled selected><%=realTimeFeedbackData.codingLanguage%></option>
                                    <option value="C">C</option>
                                    <option value="C++">C++</option>
                                    <option value="Java">Java</option>
                                    <option value="Javascript">Javascript</option>
                                    <option value="HTML/CSS">HTML/CSS</option>
                                    <option value="Python">Python</option>
                                </select>
                            </div>
                        </div>

                        <div class="form-group row">
                            <label class="col-lg-6 col-form-label">Communication : </label>
                            <div class="col-lg-6">    
                                <input type="text" class="form-control" id="communication" name="communication" >
                            </div>
                        </div>

                        <div class="form-group row">
                            <label class="col-lg-6 col-form-label">E-mail : </label>
                            <div class="col-lg-6">    
                                <input type="text" class="form-control" readonly id="email" name="email">
                            </div>
                        </div>
                        
                        <hr>
                        <div class="form-group row">
                            <label class="col-lg-3">Thinking Speed : </label>
                            <div class="col-lg-9" >
                                <input type="radio" class="col-lg-1" id="thinkingSpeed1" name="thinkingSpeed" value="1" oninput="thinkingSpeedRatingChange(this)">
                                <label class="col-lg-1" for="thinkingSpeed1">1</label>
                                <input type="radio" class="col-lg-1" id="thinkingSpeed2" name="thinkingSpeed" value="2" oninput="thinkingSpeedRatingChange(this)">
                                <label class="col-lg-1" for="thinkingSpeed2">2</label>
                                <input type="radio" class="col-lg-1" id="thinkingSpeed3" name="thinkingSpeed" value="3" oninput="thinkingSpeedRatingChange(this)">
                                <label class="col-lg-1" for="thinkingSpeed3">3</label>
                                <input type="radio" class="col-lg-1" id="thinkingSpeed4" name="thinkingSpeed" value="4" oninput="thinkingSpeedRatingChange(this)">
                                <label class="col-lg-1" for="thinkingSpeed4">4</label>
                                <input type="radio" class="col-lg-1" id="thinkingSpeed5" name="thinkingSpeed" value="5" oninput="thinkingSpeedRatingChange(this)">
                                <label class="col-lg-1" for="thinkingSpeed5">5</label>
                            </div>
                        </div>
                        
                        <div class="form-group row">
                            <label class="col-lg-3">Coding Speed : </label>
                            <div class="col-lg-9" >
                                <input type="radio" class="col-lg-1" id="codingSpeed1" name="codingSpeed" value="1" oninput="codingSpeedRatingChange(this)">
                                <label class="col-lg-1" for="codingSpeed1">1</label>
                                <input type="radio" class="col-lg-1" id="codingSpeed2" name="codingSpeed" value="2" oninput="codingSpeedRatingChange(this)">
                                <label class="col-lg-1" for="codingSpeed2">2</label>
                                <input type="radio" class="col-lg-1" id="codingSpeed3" name="codingSpeed" value="3" oninput="codingSpeedRatingChange(this)">
                                <label class="col-lg-1" for="codingSpeed3">3</label>
                                <input type="radio" class="col-lg-1" id="codingSpeed4" name="codingSpeed" value="4" oninput="codingSpeedRatingChange(this)">
                                <label class="col-lg-1" for="codingSpeed4">4</label>
                                <input type="radio" class="col-lg-1" id="codingSpeed5" name="codingSpeed" value="5" oninput="codingSpeedRatingChange(this)">
                                <label class="col-lg-1" for="codingSpeed5">5</label>
                            </div>
                        </div>
                        
                        <div class="form-group row">
                            <label class="col-lg-3">Code Quality : </label>
                            <div class="col-lg-9" >
                                <input type="radio" class="col-lg-1" id="codeQuality1" name="codeQuality" value="1" oninput="codeQualityRatingChange(this)">
                                <label class="col-lg-1" for="codeQuality1">1</label>
                                <input type="radio" class="col-lg-1" id="codeQuality2" name="codeQuality" value="2" oninput="codeQualityRatingChange(this)">
                                <label class="col-lg-1" for="codeQuality2">2</label>
                                <input type="radio" class="col-lg-1" id="codeQuality3" name="codeQuality" value="3" oninput="codeQualityRatingChange(this)">
                                <label class="col-lg-1" for="codeQuality3">3</label>
                                <input type="radio" class="col-lg-1" id="codeQuality4" name="codeQuality" value="4" oninput="codeQualityRatingChange(this)">
                                <label class="col-lg-1" for="codeQuality4">4</label>
                                <input type="radio" class="col-lg-1" id="codeQuality5" name="codeQuality" value="5" oninput="codeQualityRatingChange(this)">
                                <label class="col-lg-1" for="codeQuality5">5</label>
                            </div>
                        </div>
                        
                        <div class="form-group row">
                            <label class="col-lg-3">Concentration : </label>
                            <div class="col-lg-9" >
                                <input type="radio" class="col-lg-1" id="concentration1" name="concentration" value="1" oninput="concentrationRatingChange(this)">
                                <label class="col-lg-1" for="concentration1">1</label>
                                <input type="radio" class="col-lg-1" id="concentration2" name="concentration" value="2" oninput="concentrationRatingChange(this)">
                                <label class="col-lg-1" for="concentration2">2</label>
                                <input type="radio" class="col-lg-1" id="concentration3" name="concentration" value="3" oninput="concentrationRatingChange(this)">
                                <label class="col-lg-1" for="concentration3">3</label>
                                <input type="radio" class="col-lg-1" id="concentration4" name="concentration" value="4" oninput="concentrationRatingChange(this)">
                                <label class="col-lg-1" for="concentration4">4</label>
                                <input type="radio" class="col-lg-1" id="concentration5" name="concentration" value="5" oninput="concentrationRatingChange(this)">
                                <label class="col-lg-1" for="concentration5">5</label>
                            </div>
                        </div>
                        
                        <hr>
                        <div class="form-group product">

                            <div class="form-group row">
                                <label class="col-lg-3">Operating System : </label>
                                <div class="col-lg-9" >
                                    <input type="radio" class="col-lg-1" id="operatingSystem1" name="operatingSystem" value="1" oninput="operatingSystemRatingChange(this)">
                                    <label class="col-lg-1" for="operatingSystem1">1</label>
                                    <input type="radio" class="col-lg-1" id="operatingSystem2" name="operatingSystem" value="2" oninput="operatingSystemRatingChange(this)">
                                    <label class="col-lg-1" for="operatingSystem2">2</label>
                                    <input type="radio" class="col-lg-1" id="operatingSystem3" name="operatingSystem" value="3" oninput="operatingSystemRatingChange(this)">
                                    <label class="col-lg-1" for="operatingSystem3">3</label>
                                    <input type="radio" class="col-lg-1" id="operatingSystem4" name="operatingSystem" value="4" oninput="operatingSystemRatingChange(this)">
                                    <label class="col-lg-1" for="operatingSystem4">4</label>
                                    <input type="radio" class="col-lg-1" id="operatingSystem5" name="operatingSystem" value="5" oninput="operatingSystemRatingChange(this)">
                                    <label class="col-lg-1" for="operatingSystem5">5</label>
                                </div>
                            </div>
                            
                            <div class="form-group row">
                                <label class="col-lg-3">Database Management : </label>
                                <div class="col-lg-9" >
                                    <input type="radio" class="col-lg-1" id="dbms1" name="dbms" value="1" oninput="dbmsRatingChange(this)">
                                    <label class="col-lg-1" for="dbms1">1</label>
                                    <input type="radio" class="col-lg-1" id="dbms2" name="dbms" value="2" oninput="dbmsRatingChange(this)">
                                    <label class="col-lg-1" for="dbms2">2</label>
                                    <input type="radio" class="col-lg-1" id="dbms3" name="dbms" value="3" oninput="dbmsRatingChange(this)">
                                    <label class="col-lg-1" for="dbms3">3</label>
                                    <input type="radio" class="col-lg-1" id="dbms4" name="dbms" value="4" oninput="dbmsRatingChange(this)">
                                    <label class="col-lg-1" for="dbms4">4</label>
                                    <input type="radio" class="col-lg-1" id="dbms5" name="dbms" value="5" oninput="dbmsRatingChange(this)">
                                    <label class="col-lg-1" for="dbms5">5</label>
                                </div>
                            </div>
                        
                            <div class="form-group row">
                                <label class="col-lg-3">Datastructures : </label>
                                <div class="col-lg-9" >
                                    <input type="radio" class="col-lg-1" id="dataStructures1" name="dataStructure" value="1" oninput="dataStructuresRatingChange(this)">
                                    <label class="col-lg-1" for="dataStructures1">1</label>
                                    <input type="radio" class="col-lg-1" id="dataStructures2" name="dataStructure" value="2" oninput="dataStructuresRatingChange(this)">
                                    <label class="col-lg-1" for="dataStructures2">2</label>
                                    <input type="radio" class="col-lg-1" id="dataStructures3" name="dataStructure" value="3" oninput="dataStructuresRatingChange(this)">
                                    <label class="col-lg-1" for="dataStructures3">3</label>
                                    <input type="radio" class="col-lg-1" id="dataStructures4" name="dataStructure" value="4" oninput="dataStructuresRatingChange(this)">
                                    <label class="col-lg-1" for="dataStructures4">4</label>
                                    <input type="radio" class="col-lg-1" id="dataStructures5" name="dataStructure" value="5" oninput="dataStructuresRatingChange(this)">
                                    <label class="col-lg-1" for="dataStructures5">5</label>
                                </div>
                            </div>
                            
                            <div class="form-group row">
                                <label class="col-lg-3">Algorithms : </label>
                                <div class="col-lg-9" >
                                    <input type="radio" class="col-lg-1" id="algorithm1" name="algorithm" value="1" oninput="algorithmRatingChange(this)">
                                    <label class="col-lg-1" for="algorithm1">1</label>
                                    <input type="radio" class="col-lg-1" id="algorithm2" name="algorithm" value="2" oninput="algorithmRatingChange(this)">
                                    <label class="col-lg-1" for="algorithm2">2</label>
                                    <input type="radio" class="col-lg-1" id="algorithm3" name="algorithm" value="3" oninput="algorithmRatingChange(this)">
                                    <label class="col-lg-1" for="algorithm3">3</label>
                                    <input type="radio" class="col-lg-1" id="algorithm4" name="algorithm" value="4" oninput="algorithmRatingChange(this)">
                                    <label class="col-lg-1" for="algorithm4">4</label>
                                    <input type="radio" class="col-lg-1" id="algorithm5" name="algorithm" value="5" oninput="algorithmRatingChange(this)">
                                    <label class="col-lg-1" for="algorithm5">5</label>
                                </div>
                            </div>

                        </div>
                        <hr>
                        
                        <div class="form-group row">
                            <label class="col-lg-3">Apptitude : </label>
                            <div class="col-lg-9" >
                                <input type="radio" class="col-lg-1" id="apptitude1" name="apptitude" value="1" oninput="apptitudeRatingChange(this)">
                                <label class="col-lg-1" for="apptitude1">1</label>
                                <input type="radio" class="col-lg-1" id="apptitude2" name="apptitude" value="2" oninput="apptitudeRatingChange(this)">
                                <label class="col-lg-1" for="apptitude2">2</label>
                                <input type="radio" class="col-lg-1" id="apptitude3" name="apptitude" value="3" oninput="apptitudeRatingChange(this)">
                                <label class="col-lg-1" for="apptitude3">3</label>
                                <input type="radio" class="col-lg-1" id="apptitude4" name="apptitude" value="4" oninput="apptitudeRatingChange(this)">
                                <label class="col-lg-1" for="apptitude4">4</label>
                                <input type="radio" class="col-lg-1" id="apptitude5" name="apptitude" value="5" oninput="apptitudeRatingChange(this)">
                                <label class="col-lg-1" for="apptitude5">5</label>
                            </div>
                        </div>
                        
                        <div class="form-group row">
                            <label class="col-lg-3">Personality and Behaviour : </label>
                            <div class="col-lg-9" >
                                <input type="radio" class="col-lg-1" id="personality1" name="personality" value="1" oninput="personalityRatingChange(this)">
                                <label class="col-lg-1" for="personality1">1</label>
                                <input type="radio" class="col-lg-1" id="personality2" name="personality" value="2" oninput="personalityRatingChange(this)">
                                <label class="col-lg-1" for="personality2">2</label>
                                <input type="radio" class="col-lg-1" id="personality3" name="personality" value="3" oninput="personalityRatingChange(this)">
                                <label class="col-lg-1" for="personality3">3</label>
                                <input type="radio" class="col-lg-1" id="personality4" name="personality" value="4" oninput="personalityRatingChange(this)">
                                <label class="col-lg-1" for="personality4">4</label>
                                <input type="radio" class="col-lg-1" id="personality5" name="personality" value="5" oninput="personalityRatingChange(this)">
                                <label class="col-lg-1" for="personality5">5</label>
                            </div>
                        </div>
                        
                        <div class="form-group row">
                            <label class="col-lg-3">Projects / Resume : </label>
                            <div class="col-lg-9" >
                                <input type="radio" class="col-lg-1" id="project1" name="project" value="1" oninput="projectRatingChange(this)">
                                <label class="col-lg-1" for="project1">1</label>
                                <input type="radio" class="col-lg-1" id="project2" name="project" value="2" oninput="projectRatingChange(this)">
                                <label class="col-lg-1" for="project2">2</label>
                                <input type="radio" class="col-lg-1" id="project3" name="project" value="3" oninput="projectRatingChange(this)">
                                <label class="col-lg-1" for="project3">3</label>
                                <input type="radio" class="col-lg-1" id="project4" name="project" value="4" oninput="projectRatingChange(this)">
                                <label class="col-lg-1" for="project4">4</label>
                                <input type="radio" class="col-lg-1" id="project5" name="project" value="5" oninput="projectRatingChange(this)">
                                <label class="col-lg-1" for="project5">5</label>
                            </div>
                        </div>
                        
                        <hr>

                        <div class="form-group row">
                            <label class="col-lg-3">Overall Analysis:</label>
                            <div class="col-lg-9">    
                                <select id="overallAnalysis" class="form-control overallAnalysis" name="overallAnalysis">
                                <option value="" disabled selected>-Select-</option>
                                <option value="Srongly Proceed">Srongly Proceed</option>
                                <option value="Proceed">Proceed</option>
                                <option value="Reject">Reject</option>
                                <option value="Strongly Reject">Strongly Reject</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group row">
                            <label class="col-lg-3">Average rating:</label>
                            <span class="col-lg-3" id="avgRating"></span></p>
                        </div>

                        <hr>
                        
                        
                    </form>
                </div>
            </div>
        </div>
        <div id="meet"></div>
    </div>
    
    

    <div class="lower">
        <div class="card">

            <div class="card-header header-elements-inline">
                <!-- <h2 class="card-title">Detailed Feedback Form</h2> -->
                
            <h2 class="card-title" style="margin-left: 0;font-weight: 900;">Detailed Feedback Form</h2>

                <div class="header-elements">
                    <div class="list-icons">
                        <a href="/detailedFeedbackForm?meetLink=<%=link%>&uniqueId=<%=uniqueId%>" " target="_blank"><button type="button" class="btn btn-light">Fill later</button></a>
                    </div>
                </div>
            </div>

            <br>

            <div class="card-body">
                <form action="">
                    
                    <fieldset>
                        <legend class="font-weight-bold">
                            <i class="icon-reading mr-2"></i>
                            Introduction
                            <a class="float-right text-body" data-toggle="collapse" data-target="#demo1">
                                <i class="icon-circle-down2"></i>
                            </a>
                        </legend>

                        <div class="collapse show" id="demo1">
                            <div class="form-group row">
                                <label class="col-lg-4 col-form-label">Languages: </label>
                                <div class="col-lg-8">
                                    <input type="text" class="form-control" id="language" placeholder="" oninput="languageChange(this)">
                                </div>
                            </div>
                            
                            <div class="form-group row">
                                <label class="col-lg-4 col-form-label">Prior Experience:</label>
                                <div class="col-lg-8">
                                    <input type="number" class="form-control" id="priorExperience" placeholder="(in months)" oninput="priorExperienceChange(this)">
                                </div>
                            </div>

                            <div class="form-group row">
                                <label class="col-lg-4 col-form-label">Projects:</label>
                                <div class="col-lg-8">
                                    <input type="text" class="form-control" id="projects" placeholder="" oninput="detailProjectChange(this)">
                                </div>
                            </div>

                            <div class="form-group row">
                                <label class="col-lg-4 col-form-label">Tech Stack:</label>
                                <div class="col-lg-8">
                                    <input type="text" class="form-control" id="techStack" placeholder="" oninput="techStackChange(this)">
                                </div>
                            </div>

                        </div>
                    </fieldset>

                    <fieldset>
                        <legend class="font-weight-bold">
                            <i class="icon-reading mr-2"></i>
                            Data Structure and Algorithm
                            <a class="float-right text-body" data-toggle="collapse" data-target="#demo2">
                                <i class="icon-circle-down2"></i>
                            </a>
                        </legend>

                        <div class="collapse show" id="demo2">

                            <div class="row">
                                <div class="col-lg-6">
                                    <fieldset>
                                        <legend class="font-weight-semibold">
                                            <i class="icon-file-text2 mr-2"></i>
                                            Question 1
                                        </legend>
                
                                        
                                            <div class="form-group row">
                                                <label class="col-lg-4 col-form-label">Question</label>
                                                <div class="col-lg-8">
                                                    <select class="form-control" id="dsaQuestion1" placeholder="" oninput="dsaQuestion1Change(this)">
                                                        <option selected disabled>Select an option</option>
                                                        <% for (var i = 0; i < questionData.length; i++) { %>
                                                            <% if (questionData[i].categoryId == "1") { %>
                                                              <option><%= questionData[i].question %></option>
                                                            <% } %>
                                                          <% } %>
                                                    </select>
                                                </div>
                                            </div>
                
                                            <div class="form-group row">
                                                <label class="col-lg-4 col-form-label">Answer:</label>
                                                <div class="col-lg-8">
                                                    <textarea rows="5" cols="5" class="form-control" id="dsaAnswer1" placeholder="Enter detailed answer here" oninput="dsaAnswer1Change(this)"></textarea>
                                                </div>
                                            </div>
                                        
                                    </fieldset>
                                </div>

                                <div class="col-lg-6">
                                    <fieldset>
                                        <legend class="font-weight-semibold">
                                            <i class="icon-file-text2 mr-2"></i>
                                            Question 2
                                        </legend>
                
                                        
                                            <div class="form-group row">
                                                <label class="col-lg-4 col-form-label">Question</label>
                                                <div class="col-lg-8">
                                                    <select class="form-control" id="dsaQuestion2" placeholder="" oninput="dsaQuestion2Change(this)">
