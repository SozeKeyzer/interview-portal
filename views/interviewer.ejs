    <style>
        body 
        {
            overflow: scroll;
        }
        .upper 
        {
            width: 100%;
            display: flex;
            height: 70vh;
            
            /* border: 2px solid red; */
            /* background-color: chartreuse; */
        }
        .realTime 
        {
            height: 100%;
            width: 35%;
            float: left;
            border: 5px solid black;
        }
